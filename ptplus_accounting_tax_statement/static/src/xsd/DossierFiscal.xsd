      <!-- Quadro 03 - Determinação do limite da perda por imparidade -->
      <xs:element name="Q03-Modelo30" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Q03-Modelo30-Linhas">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="Q03-Modelo30-Linha" maxOccurs="7" minOccurs="1">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="Q03-01">
                          <xs:annotation>
                            <xs:documentation>
                              Descrição tipificada da linha do Quadro 3 referente a Determinação do limite da perda por imparidade
                            </xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:enumeration value="Créditos em Contencioso - Relativos a processos de insolvência e recuperação de empresas"/>
                              <xs:enumeration value="Créditos em Contencioso - Relativos a processos de execução"/>
                              <xs:enumeration value="Créditos em Contencioso - Reclamados judicialmente"/>
                              <xs:enumeration value="Créditos em Mora - Mais de 6 e até 12 meses"/>
                              <xs:enumeration value="Créditos em Mora - Mais de 12 e até 18 meses"/>
                              <xs:enumeration value="Créditos em Mora - Mais de 18 e até 24 meses"/>
                              <xs:enumeration value="Créditos em Mora - Mais de 24 meses"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                        <xs:element name="Q03-02" type="Montante16Type">
                          <xs:annotation>
                            <xs:documentation>Valor</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="Q03-03" type="Montante16Type">
                          <xs:annotation>
                            <xs:documentation>Limite legal (Q03-03)=(Q03-02)*)*(Percentagem(2))</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="Q03-04" type="Montante16Type" minOccurs="0">
                          <xs:annotation>
                            <xs:documentation>Valor a acrescer</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
