  <!-- Estrutura do Modelo 31-->
  <xs:complexType name="AnexoModelo31Type">
    <xs:sequence>
      <!-- Quadro 01 -->
      <xs:element name="Q01-Modelo31-Cabecalho">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Q01-Natureza">
              <xs:simpleType>
                <xs:restriction base="xs:byte">
                  <xs:enumeration value="1">
                    <xs:annotation>
                      <xs:documentation>Activos Fixos Tangíveis</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="2">
                    <xs:annotation>
                      <xs:documentation>Activos Intangíveis</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="3">
                    <xs:annotation>
                      <xs:documentation>Propriedades de Investimento</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="4">
                    <xs:annotation>
                      <xs:documentation>Activos Biológicos Não Consumiveis</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="5">
                    <xs:annotation>
                      <xs:documentation>Partes de Capital</xs:documentation>
                    </xs:annotation>
                  </xs:enumeration>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <!-- Quadro 02 -->
      <xs:element name="Q02-Modelo31-Detalhe">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Q02-Modelo31-Linhas">
              <xs:complexType>
