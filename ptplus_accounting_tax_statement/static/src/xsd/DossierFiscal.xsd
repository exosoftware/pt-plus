                            <xs:documentation>Saldo para o período seguinte (Q02-10)=(Q02-02)-(Q02-05)-(Q02-06)-(Q02-07)-(Q02-08)+(Q02-09)</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="Q02-Modelo30-Somas_II">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="Q02-Coluna02" type="Montante16Type"/>
                  <xs:element name="Q02-Coluna03" type="Montante16Type"/>
                  <xs:element name="Q02-Coluna04" type="Montante16Type"/>
                  <xs:element name="Q02-Coluna05" type="Montante16Type"/>
                  <xs:element name="Q02-Coluna06" type="Montante16Type"/>
                  <xs:element name="Q02-Coluna07" type="Montante16Type"/>
                  <xs:element name="Q02-Coluna08" type="Montante16Type"/>
                  <xs:element name="Q02-Coluna09" type="Montante16Type"/>
                  <xs:element name="Q02-Coluna10" type="Montante16Type"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      
      <!-- Quadro 02.2 - Provisões fiscalmente dedutíveis -->
      <xs:element name="Q02-Modelo30-Detalhe_III" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Q02-Modelo30-Linhas_III">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="Q02-Modelo30-Linha_III" maxOccurs="unbounded" minOccurs="1">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="Q02-01" type="xs:string">
