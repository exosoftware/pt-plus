                            </xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:enumeration value="CRÉDITOS EM CONTENCIOSO"/>
                              <xs:enumeration value="CRÉDITOS EM MORA"/>
                              <xs:enumeration value="AJUSTAMENTOS EM INVENTÁRIOS"/>
