                        <xs:element name="Q02-02" type="Montante16Type">
                          <xs:annotation>
                            <xs:documentation>TOTAL do Saldo do período anterior</xs:documentation>
                          </xs:annotation>
                        </xs:element>
                        <xs:element name="Q02-03" type="Montante16Type">
