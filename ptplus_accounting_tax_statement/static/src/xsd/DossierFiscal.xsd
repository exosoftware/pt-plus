                              <xs:enumeration value="Subprodutos, desperdícios, resíduos e refugos"/>
                              <xs:enumeration value="Produtos e trabalhos em curso"/>
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:element>
                        <xs:element name="Q04-02" type="Montante16Type">
                          <xs:annotation>
