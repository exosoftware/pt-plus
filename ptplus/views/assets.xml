<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <template id="report_assets_common" inherit_id="web.report_assets_common">
        <xpath expr="." position="inside">
            <link
                rel="stylesheet"
                type="text/scss"
                href="/ptplus/static/src/scss/layout.scss"
            />
        </xpath>
    </template>

    <template id="assets_backend" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link
                rel="stylesheet"
                type="text/scss"
                href="/ptplus/static/src/scss/backend.scss"
            />
            <script
                type="text/javascript"
                src="/ptplus/static/src/js/web_client.js"
            />
            <script
                type="text/javascript"
                src="/ptplus/static/src/js/widgets/notification.js"
            />
        </xpath>
    </template>

</odoo>
